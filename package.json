{
  "name": "@klevio/export-to-csv",
  "version": "0.2.3",
  "description": "Easily create CSV data from json collection",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klevio/export-to-csv.git"
  },
  "scripts": {
    "dev": "tsc --watch",
    "test": "tsc && karma start",
    "compile": "npm run compile-es && npm run compile-js",
    "clean": "npm run clean-es && npm run clean-js",
    "compile-es": "tsc --outDir es --module es6",
    "clean-es": "rm -rf es",
    "compile-js": "tsc",
    "clean-js": "rm -rf build",
    "prepublish": "npm run compile"
  },
  "keywords": [
    "export-to-csv",
    "export-to-excel",
    "csv",
    "excel",
    "libreoffice",
    "openoffice",
    "angular2",
    "typescript",
    "react",
    "ngx",
    "angular4",
    "angular5"
  ],
  "author": "klevio",
  "email": "info@klevio.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/klevio/export-to-csv/issues"
  },
  "main": "./build/index.js",
  "module": "./es/index.js",
  "types": "./build/export-to-csv.d.ts",
  "homepage": "https://github.com/klevio/export-to-csv#readme",
  "devDependencies": {
    "@types/jasmine": "^3.5.0",
    "es6-shim": "^0.35.3",
    "jasmine-core": "^3.6.0",
    "karma": "^5.0.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^4.0.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^5.2.0",
    "typescript": "~3.9.0"
  },
  "dependencies": {}
}
